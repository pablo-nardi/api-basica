#configuracion general (se puede hacer en cualquier momento)

#datos del usuario
git config --global user.name "nombre_usuario"
git config --global user.email "email_id"

#crear rama
git checkout -b "nombre_nueva_rama"

#cambiar de rama
git checkout "nombre_rama"

#borrar rama
git checkout "nombre_rama"

#///////////////////////////

#hacer init
git init


#clonar repo de la nube
git clone linkdelrepo

#editar le que queremos

#hacer un add
git add fyle_name.type

#comitear
git commit -m "mensaje"

#hacer push
git push origin "nombre rama"
#capas si no funciona hay que hacer un:
git remote add origin url

#/////////////////////